{
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  },
  "plugins": ["prettier", "react"],
  "rules": {
    "max-len": ["error", 120],
    "prettier/prettier": "error"
  },
  "env": {
    "es6": true
  }
}